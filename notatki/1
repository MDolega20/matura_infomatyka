def fib_rek(n):
    """
        Funkcja zwraca n-ty wyraz ciÄ…gu Fibonacciego.
        Wersja rekurencyjna.
    """
    if n < 1:
        return 0
    if n < 2:
        return 1
    return fib_rek(n - 1) + fib_rek(n - 2)

def nwd(a, b):
    while b:
        a, b = b, a%b
    return a

def nwd(a,b):
    while a != b:
        if a > b:
            a -= b
        else:
            b -= a
    return a

